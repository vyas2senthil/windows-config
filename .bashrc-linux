#!/bin/bash

export PATH=~/gcode/guilt:~/bin/linux/ext/jdk/bin:$HOME/bin/linux:$HOME/bin:$HOME/bin/linux/ext/`uname -m`:$HOME/bin/linux/ext:/usr/local/bin:$PATH
export PATH=$PATH:$HOME/bin/linux/ext/android-sdk-linux_86/tools:/usr/local/bin:/sbin:/usr/sbin:/usr/local/sbin

export CLASSPATH=/usr/share/java/stringtemplate.jar:/usr/share/java/antlr3.jar:/usr/share/java/antlr3-runtime.jar:$HOME/bin/linux/ext/antlrworks.jar:.



alias minicom='minicom -w -D /dev/ttyUSB*'
if [ -n "$BASH_VERSION" -a "$BASH" = /bin/bash ]; then
    . ~/.bashrc-linux-bash
fi

. ~/.bashrc-aliases
if test -e ~/.bashrc-linux-$USER; then
    . ~/.bashrc-linux-$USER
fi


