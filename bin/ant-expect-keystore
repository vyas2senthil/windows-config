#!/usr/bin/expect -f

spawn ant $argv

set fl [open "|get-authinfo localhost ant"]
set password [read $fl]
close $fl

expect -re "Please enter.*password.*\n" {
    puts "send password\n";
    stty -echo
    send "$password\n";
    stty echo
    exp_continue;
} timeout {
    puts "time out\n"
}

exit [lindex [wait] 3]

