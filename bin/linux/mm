#!/bin/bash
. ~/.bashrc
set -e
set -o pipefail
build_env=`lookup_file build/envsetup.sh`
top_dir=`dirname $build_env`
top_dir=`readlink -f $top_dir/..`

cd $top_dir/
. build/envsetup.sh
cd - 

lcd `lookup_file Android.mk` >/dev/null 2>&1
mm "$@" 2>&1|grep -v '^Copy: out/target/product/.*/system/media/audi'|perl -ne 'print unless /unmappable character for encoding ascii/'
ainstall

echo OK
