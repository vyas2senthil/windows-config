#!/bin/bash
. ~/.bashrc
set -e
build_env=`lookup_file build/envsetup.sh`
top_dir=`dirname $build_env`
top_dir=`readlink -f $top_dir/..`

if test -f $top_dir/TARGET_PRODUCT; then
    . $top_dir/TARGET_PRODUCT
fi

cd $top_dir/
. build/envsetup.sh
cd - 

lcd `lookup_file Android.mk`
mm
ainstall

echo OK
