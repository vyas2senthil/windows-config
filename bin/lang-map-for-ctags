#!/usr/bin/perl

use strict;

while (<>) {
    if (/plugin-example\|/) {
	while(<>) {
	    if (m/:langmap=(.*?)\\?:(.*):\\?$/) {
		print "$1:$2,";
	    }
	}
    }
}


