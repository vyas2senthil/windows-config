#!/bin/bash

ps.pl $1|grep -v ps-uptime|
perl -e '
<>; #get rid of the first header line
while (<>) {
    @F = split;
    if ($F[2] =~ m/(\d*?)?:?(\d*?)?:?(\d+)$/) { 
        if ($1 * 3600 + $2 * 60 + $3 > '$2') {
            print $F[0]; 
            exit 0;
        }
    }
    exit -1;
}
exit -1;
'

