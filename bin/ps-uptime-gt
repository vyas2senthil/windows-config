#!/bin/bash

ps -eo pid,etime,cmd|grep $1|grep -v -e "grep $1"|grep -v -P "^\s*$$\b" |
perl -e '
while (<>) {
    @F = split;
    if ($F[1] =~ m/(\d*?)?:?(\d*):(\d*)$/) { 
        if ($1 * 3600 + $2 * 60 + $3 > '$2') {
            print $F[0]; 
            exit 0;
        }
    }
    exit -1;
}
exit -1;
'

