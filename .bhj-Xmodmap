! The Alt_R is now right Windows key
keycode 108 = Super_R

! The "Caps Lock" is now Control_L
keycode 66 = Control_L

! The scroll_clok is now caps_lock
keycode 78 = Caps_Lock

keysym Super_L = Super_R

clear lock
clear mod1
clear mod4
clear control

add mod1 = Alt_L
add control = Control_L Control_R
add lock = Caps_Lock
add mod4 = Super_R
! This is to make sawfish happy. Seems it will constantly do weird thing with Hyper/Super
keysym Super_R = Super_L
