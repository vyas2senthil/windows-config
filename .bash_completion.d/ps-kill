#!/bin/bash
_ps_list()
{
    local IFS=$'\n'
    COMPREPLY=( $(skeleton_compgen_word.pl -d '\n' -- "$(ps -u $USER -o pid,cmd)" "$cur") )
}

_ps_kill()
{
    local cmd cur prev skinsdir IFS=$' \t\n' i j k=0

    COMPREPLY=()
    cmd=${COMP_WORDS[0]}
    _get_comp_words_by_ref cur prev

    case $COMP_CWORD in
        *)
	    _ps_list
            return 0
            ;;
    esac

    return 0
}

complete -o filenames -F _ps_kill ps-kill
